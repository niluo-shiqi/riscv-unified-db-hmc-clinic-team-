grammar org.xtext.clinic.Riscv with org.eclipse.xtext.common.Terminals

generate riscv "http://www.xtext.org/clinic/Riscv"

Model:
	csrs += Csr*;

/* Taken from Ajit */
Csr:
	'csr' name=ID
		description=STRING
		address=HEX
		reset_value=STRING
		';';
		
/* Terminals */
terminal HEX: 
	('0x' | '0X')
	('0'..'9' | 'A'..'F' | 'a'..'f')
	(('0'..'9' | 'A'..'F' | 'a'..'f' | '_')*('0'..'9' | 'A'..'F' | 'a'..'f'))?
;